<%= content_for(:title) do %>
  Digicode
<% end %>

<%= content_for(:description) do %>
  Browse users and add people as friends to share your digicodes
<% end %>

<div class="container">
  <div class="row">
    <div class="col-xs-12 padding-sides-15">
      <% @users.each do |user| %>
        <%= render 'user', user: user %>
      <% end %>
    </div>
  </div>
</div>